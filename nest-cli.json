{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/csv-uploader/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/csv-uploader/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/csv-uploader",
  "projects": {
    "csv-uploader": {
      "type": "application",
      "root": "apps/csv-uploader",
      "entryFile": "main",
      "sourceRoot": "apps/csv-uploader/src",
      "compilerOptions": {
        "tsConfigPath": "apps/csv-uploader/tsconfig.app.json"
      }
    },
    "auth": {
      "type": "application",
      "root": "apps/auth",
      "entryFile": "main",
      "sourceRoot": "apps/auth/src",
      "compilerOptions": {
        "tsConfigPath": "apps/auth/tsconfig.app.json"
      }
    },
    "import-transaction": {
      "type": "application",
      "root": "apps/import-transaction",
      "entryFile": "main",
      "sourceRoot": "apps/import-transaction/src",
      "compilerOptions": {
        "tsConfigPath": "apps/import-transaction/tsconfig.app.json"
      }
    },
    "rabbitmq": {
      "type": "library",
      "root": "libs/rabbitmq",
      "entryFile": "index",
      "sourceRoot": "libs/rabbitmq/src",
      "compilerOptions": {
        "tsConfigPath": "libs/rabbitmq/tsconfig.lib.json"
      }
    }
  }
}